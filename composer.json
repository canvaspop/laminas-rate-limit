    "laminas/laminas-eventmanager": "3.4.*",
    "laminas/laminas-cache-storage-adapter-memory": "2.0.*",
    "laminas/laminas-cache-storage-adapter-memcached": "2.0.*",
    "laminas/laminas-http": "2.15.*",
